{
  "kind": "collectionType",
  "collectionName": "user_ads",
  "info": {
    "name": "UserAds",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 5,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "content": {
      "type": "richtext",
      "required": true,
      "minLength": 5,
      "maxLength": 1000,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 10,
      "maxLength": 300,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "phoneNumber": {
      "type": "string",
      "required": true,
      "minLength": 5,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subcategories": {
      "via": "user_ad",
      "collection": "subcategory"
    },
    "ad_board_user": {
      "model": "ad-board-users",
      "via": "user_ads"
    },
    "city": {
      "type": "enumeration",
      "enum": [
        "Helsinki",
        "Vantaa",
        "Espoo",
        "Oulu",
        "Turku",
        "Tampere",
        "Rovaniemi",
        "Jyvaskyla",
        "Kuopio",
        "Lahti",
        "Pori",
        "Kouvola",
        "Joensuu",
        "Lappeenranta",
        "Hameenlinna",
        "Vaasa"
      ],
      "required": false,
      "default": "Helsinki",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sub_section": {
      "via": "user_ads",
      "model": "sub-section"
    }
  }
}
